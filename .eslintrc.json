{
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "env": {
    "node": true
  },
  "extends": [
    "eslint:recommended"
  ],
  "rules": {
    "indent": ["error", 2],
    "no-mixed-spaces-and-tabs":"error",
    "comma-dangle": ["error", "always-multiline"],
    "arrow-body-style": 0,
    "linebreak-style": 0,
    "func-names": 0,
    "camelcase": 0
  }
}
